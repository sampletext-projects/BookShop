<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="650.0" prefWidth="650.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="fx.controllers.SignInAsSellerScreenController">
    <children>
        <ImageView fitHeight="756.0" fitWidth="650.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../media/SignInScreen.png"/>
            </image>
        </ImageView>
        <TextField fx:id="textFieldSellerLogin" layoutX="229.0" layoutY="291.0" prefHeight="40.0" prefWidth="296.0">
            <font>
                <Font name="French Script MT" size="20.0"/>
            </font>
        </TextField>
        <PasswordField fx:id="passwordFieldSeller" layoutX="229.0" layoutY="364.0" prefHeight="40.0" prefWidth="296.0">
            <font>
                <Font name="French Script MT" size="20.0"/>
            </font>
        </PasswordField>
        <Button layoutX="362.0" layoutY="435.0" mnemonicParsing="false" onAction="#onButtonSignInAsSellerClick"
                opacity="0.0" prefHeight="73.0" prefWidth="181.0" text="Sign in as seller"/>
    </children>
</AnchorPane>
