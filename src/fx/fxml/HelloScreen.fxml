<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="650.0" prefWidth="650.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="fx.controllers.HelloScreenController">
    <children>
        <ImageView fitHeight="700.0" fitWidth="650.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../media/HelloScreen1.png"/>
            </image>
        </ImageView>
        <Button onAction="#onButtonSignInClick" layoutX="41.0" layoutY="277.0" mnemonicParsing="false" opacity="0.0"
                prefHeight="103.0" prefWidth="176.0" text="Sign in"/>
        <Button onAction="#onButtonSignUpClick" layoutX="432.0" layoutY="277.0" mnemonicParsing="false" opacity="0.0"
                prefHeight="103.0" prefWidth="176.0" text="Sign up"/>
        <Button onAction="#onButtonSignInAsSellerClick" layoutX="165.0" layoutY="435.0" mnemonicParsing="false"
                opacity="0.0" prefHeight="148.0" prefWidth="320.0" text="Sign in as seller"/>
    </children>
</AnchorPane>
